//#######################PROGAMA1########################
#include <stdio.h>
int main (void){

float peso, altura, imc;

 printf("Qual a sua altura ?");
 scanf("%f",&altura);
 printf("Qual o seu peso ?");
 scanf("%f",&peso);

  imc = peso / (altura * altura);

  printf("O seu peso é: %.2f KG\n",peso);
  printf("A sua altura: %.2f \n",altura);
  printf("O seu  imc é: %.2f\n",imc);

}